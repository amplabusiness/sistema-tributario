# 🌟 VARIÁVEIS DE AMBIENTE - FRONTEND VITE
# Copie este arquivo para .env.local e configure suas variáveis

# 🌟 API BACKEND
VITE_API_URL=https://api.conttatax.com.br
VITE_WS_URL=wss://api.conttatax.com.br/ws

# 🌟 AMBIENTE
VITE_NODE_ENV=production
VITE_APP_VERSION=1.0.0

# 🌟 MONITORAMENTO
VITE_MONITORING_ENABLED=true
VITE_LOG_LEVEL=info

# 🌟 SEGURANÇA
VITE_CSP_NONCE=your-csp-nonce-here
VITE_FEATURE_FLAGS=real-time,3d-charts,ai-agents

# 🌟 PERFORMANCE
VITE_CACHE_TTL=300
VITE_MAX_RETRIES=3
VITE_TIMEOUT=10000

# 🌟 DESENVOLVIMENTO (não usar em produção)
# VITE_API_URL=http://localhost:3001
# VITE_WS_URL=ws://localhost:3001/ws
# VITE_NODE_ENV=development 